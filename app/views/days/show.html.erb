<div id="day" data-url="<%= day_path @day %>">
  <h1><%= @day.created_at.strftime("%A, %b %d, %Y") %></h1>
  <% @times.each_slice(2) do |hour| %>
      <div class="time">
        <%= display_time(hour.first) %>
        <div>
          <% hour.each do |time| %>
            <a href="" data-time="<%= time %>" class="time-set <%= @day[time] %>"></a>
          <% end %>
        </div>
      </div>
  <% end %>
  <table border="0" cellspacing="0" cellpadding="3" width="100%">
    <tr>
      <td width="50%"><span class="key sleep"></span> Sleep</td>
      <td width="50%"><span class="key work"></span> Work</td>
    </tr>
    <tr>
      <td width="50%"><span class="key personal"></span> Personal</td>
      <td width="50%"><span class="key family"></span> Family</td>
    </tr>
    <tr>
      <td width="50%"><span class="key friends"></span> Friends</td>
      <td width="50%"><span class="key wasted"></span> Wasted</td>
    </tr>
  </table>
</div>


